{
  "name": "@paycertify/eslint-plugin",
  "version": "0.11.1",
  "description": "ESLint plugin for javascript projects",
  "repository": "https://github.com/PayCertify/eslint-plugin",
  "private": true,
  "main": "lib/index.js",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "paycertify"
  ],
  "scripts": {
    "release": "release-it",
    "test": "jest"
  },
  "release-it": {
    "git": {
      "tagName": "v${version}",
      "commitMessage": "v${version}",
      "pushArgs": "--follow-tags"
    },
    "npm": {
      "publish": false
    },
    "github": {
      "release": true
    }
  },
  "dependencies": {
    "babel-eslint": "10.0.3",
    "eslint-plugin-ember": "7.7.1",
    "eslint-plugin-node": "10.0.0"
  },
  "devDependencies": {
    "eslint": "~6.7.2",
    "glob": "^7.1.6",
    "jest": "^24.9.0",
    "release-it": "^12.4.3"
  },
  "peerDependencies": {
    "eslint": "^6.7.0"
  },
  "engines": {
    "node": ">=0.10.0"
  }
}
