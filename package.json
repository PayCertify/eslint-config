{
  "name": "@paycertify/eslint-plugin",
  "version": "0.8.0",
  "description": "ESLint plugin for javascript projects",
  "repository": "https://github.com/PayCertify/eslint-plugin",
  "private": true,
  "main": "lib/index.js",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "paycertify"
  ],
  "scripts": {
    "release": "release-it"
  },
  "release-it": {
    "git": {
      "tagName": "v${version}",
      "commitMessage": "v${version}"
    },
    "npm": {
      "publish": false
    },
    "github": {
      "release": true
    }
  },
  "dependencies": {
    "babel-eslint": "10.0.3",
    "eslint-plugin-ember": "7.0.0",
    "eslint-plugin-node": "10.0.0"
  },
  "devDependencies": {
    "eslint": "^6.4.0",
    "release-it": "^12.4.0"
  },
  "peerDependencies": {
    "eslint": "^6.2.0"
  },
  "engines": {
    "node": ">=0.10.0"
  }
}
