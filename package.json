{
  "name": "@paycertify/eslint-plugin",
  "version": "0.16.0",
  "description": "ESLint plugin for javascript projects",
  "repository": "https://github.com/PayCertify/eslint-plugin",
  "private": true,
  "main": "lib/index.js",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "paycertify"
  ],
  "scripts": {
    "release": "release-it",
    "test": "jest"
  },
  "release-it": {
    "git": {
      "tagName": "v${version}",
      "commitMessage": "v${version}",
      "pushArgs": "--follow-tags"
    },
    "npm": {
      "publish": false
    },
    "github": {
      "release": true
    }
  },
  "dependencies": {
    "acorn": "^8.0.4",
    "babel-eslint": "10.1.0",
    "eslint-plugin-ember": "10.1.2",
    "eslint-plugin-json": "2.1.2",
    "eslint-plugin-node": "11.1.0",
    "minimist": ">=1.2.2"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "glob": "^7.1.6",
    "jest": "^27.0.3",
    "release-it": "^14.2.2"
  },
  "peerDependencies": {
    "eslint": "^6.7.0"
  },
  "engines": {
    "node": ">=0.10.0"
  }
}
